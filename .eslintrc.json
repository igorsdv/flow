{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-typescript",
        "plugin:@typescript-eslint/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-shadow": "off",
        "import/prefer-default-export": "off"
    }
}
